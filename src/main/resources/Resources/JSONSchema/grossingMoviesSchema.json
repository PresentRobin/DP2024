{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "HighestGrossingMovies",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "movieId": { "type": "string" },
    "title": { "type": "string" },
    "lifetimeGross": { "type": "number" },
    "year": { "type": "integer" },
    "studio": { "type": "string" },
    "rating": { "type": "string" },
    "runtime": { "type": "number" },
    "budget": { "type": "number" },
    "releaseDate": { "type": "string", "format": "date" },
    "voteAvg": { "type": "number" },
    "voteCount": { "type": "integer" },
    "franchiseId": { "type": "integer" }
  },
  "required": ["movieId", "title", "lifetimeGross"]
}
